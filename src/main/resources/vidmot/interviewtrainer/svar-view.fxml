<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<DialogPane prefHeight="375.0" prefWidth="750.0" styleClass="masterContainer" stylesheets="@../../css/interview.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
    <header>
        <Label fx:id="fxSpurning" style="-fx-font-size: 16px;" text="Question" wrapText="true">
            <padding>
                <Insets bottom="8.0" left="12.0" right="12.0" top="8.0" />
            </padding>
         <font>
            <Font size="18.0" />
         </font>
        </Label>
    </header>
    <expandableContent>
    </expandableContent>
    <buttonTypes>
        <ButtonType fx:constant="OK" />
        <ButtonType fx:constant="CLOSE" />
    </buttonTypes>
    <graphic>
        <ScrollBar orientation="VERTICAL" />
    </graphic>
    <content>
        <VBox prefHeight="236.0" prefWidth="391.0">
            <children>
                <TextArea fx:id="fxTextArea" prefHeight="85.0" VBox.vgrow="ALWAYS" />
                <Label fx:id="fxFeedbackLabel" alignment="TOP_LEFT" prefHeight="135.0" prefWidth="1000.0" style="-fx-background-color: #F1EFEC; -fx-border-color: black; -fx-border-width: 3px; -fx-border-radius: 5;" wrapText="true">
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </padding>
                </Label>
            </children>
        </VBox>
    </content>
</DialogPane>
